<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.11.0//EN"
  "http://gwtproject.org/doctype/2.12.2/gwt-module.dtd">
<module rename-to="tetris">
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.canvas.Canvas'/>
  <inherits name='com.google.gwt.dom.DOM'/>
  <inherits name='org.parttio.example.TetrisLib'/>

  <source path="client"/>
  <collapse-all-properties />

  <entry-point class="org.parttio.tetrisgwt.client.TetrisGWT"/>
  <add-linker name="sso" />
</module>